meta {
  name: Create or Update Profit Margin
  type: http
  seq: 5
}

put {
  url: {{base_url}}/admin/settings/profit-margins
  body: json
  auth: bearer
}

auth:bearer {
  token: {{token}}
}

body:json {
  {
    "retail_margin": 10,
    "reseller_margin": 5
  }
}

settings {
  encodeUrl: true
  timeout: 0
}
